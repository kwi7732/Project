<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 평상시 -->
    <item android:state_focused="false" android:state_pressed="false">
        <bitmap android:src="@drawable/col6"/>
        <shape android:shape="rectangle">
            <solid android:color="#c3e7ff" />  <!--f5d1f0 -->
            <stroke android:color="#c7c7c7" android:width="0.2dp" />
            <padding android:bottom="5dp" android:left="5dp"
                android:right="5dp" android:top="5dp" />
        </shape>
    </item>

    <!-- 클릭했을 때 -->
    <item android:state_focused="false" android:state_pressed="true">
        <bitmap android:src="@drawable/col6d"/>
        <shape>
            <solid android:color="#a9c8de" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</selector>